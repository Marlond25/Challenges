# This is my solution to the challenge!

def QuestionsMarks(str):
    counter = 0
    result = False
    for i in str:

        if str[counter : counter + 1].isnumeric():
            counter_qMarks = counter
            qMarks = 0

            for j in range(len(str) - counter):

                if str[counter_qMarks : counter_qMarks + 1] == "?":
                    qMarks = qMarks + 1

                elif str[counter_qMarks : counter_qMarks + 1].isnumeric():

                    if (qMarks == 3) and (int(str[counter : counter + 1]) + int(str[counter_qMarks : counter_qMarks + 1])) == 10:
                        result = True

                counter_qMarks = counter_qMarks + 1
        counter = counter + 1
    return result

print(QuestionsMarks("arrb6??lp4x??xbl??6??eee5sDFG?sgfdsg??sG?5sgf????sgfdsddfg4fhd?GHd6???dgh5?dhd???1dhg?ds?s?s?S9??S5?02"))
