# Source: https://www.coderbyte.com/editor/Questions%20Marks:Python
# This is my solution to the challenge!

def QuestionsMarkss(str):

    questionMark = 0
    digitOne = 0
    digitTwo = 0
    counter = 0
    counter1 = 0
    lenght = len(str)
    result = ""

    for i in str:
        key = str[counter : counter + 1]

        if key.isnumeric():
            digitOne = int(key)
            x = lenght - counter - 1
            counter1 = counter + 1

            for j in range(x):

                if (str[counter1 : counter1 + 1] == "?"):
                    questionMark = questionMark + 1
                elif str[counter1 : counter1 + 1].isnumeric():
                    digitTwo = int(str[counter1 : counter1 + 1])

                    if (digitOne + digitTwo == 10) and (questionMark == 3):
                        return "true"
                    else:
                        return "false"

                counter1 = counter1 + 1

        counter = counter + 1
        counter1 = counter1 + 1

    return result

print(QuestionsMarkss("arrb6??lp4x???xbl5???eee5"))
